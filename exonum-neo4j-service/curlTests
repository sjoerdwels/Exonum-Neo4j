  "body": {
    "queries": "abort;CREATE (n)"
  },
  "protocol_version": 0,
  "service_id": 144,
  "message_id": 0,
  "signature":"9f684227f1de663775848b3db656bca685e085391e2b00b0e115679fd45443ef58a5abeb555ab3d5f7a3cd27955a2079e5fd486743f36515c8e5bea07992100b"
}'

curl -i -H "Content-Type: application/json" -X GET 127.0.0.1:8200/api/services/neo4J_blockchain/v1/transactions

